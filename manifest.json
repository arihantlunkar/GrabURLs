{
  "name": "Get URLs from Web Page",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get URLs from a web page and save to file.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>" ,"downloads"]
}